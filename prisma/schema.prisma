generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = "mysql://root:@localhost:3306/pagingname"
}

model tb_paging {
  id             Int      @id @default(autoincrement())
  belt_no        String
  flight_no      String
  name_passenger String
  created_at     DateTime @default(now())
  handle_by      String
  free_text      String
  status         Int
}


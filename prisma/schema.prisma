generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model tb_paging {
  id             Int      @id @default(autoincrement())
  belt_no        String
  flight_no      String
  name_passenger String
  last_update    DateTime @default(now())
  handle_by      String
  free_text      String
  status         Int
}

model tb_paging_log {
  id             Int      @id @default(autoincrement())
  belt_no        String
  flight_no      String
  name_passenger String
  last_update    DateTime @default(now())
  handle_by      String
  free_text      String
  status         Int
}